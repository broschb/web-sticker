.web-menu{
  background: #5cb85c;
  color: white;
  border-radius: 50%;
  box-shadow: 0 0 4px rgba(0,0,0,.14),0 4px 8px rgba(0,0,0,.28);
  outline: none;
  padding: 10px 5px 10px 5px;
  cursor: pointer;
  display:inline-block;
  width: 50px;
  height: 50px;
  text-align: center;
  vertical-align: bottom;
  margin: 2px 2px 20px 2px;
}

.web-menu:hover{
  box-shadow: 0 0 6px rgba(0,0,0,.30),0 4px 8px rgba(0,0,0,.28);
}
